data(mtcars)
head(mtcars)
str(mtcars)
summary(mtcars)
print(mtcars)
s3_obj <- list(name = "Jadyn Hollar", age = 19, GPA = 3.5)
class(s3_obj) <- "student_s3"
print(s3_obj)
setClass("student_s4",
         slots = c(name = "character", age = "numeric", GPA = "numeric"))
s4_obj <- new("student_s4", name = "Jadyn Hollar", age = 19, GPA = 3.5)
print(s4_obj)

